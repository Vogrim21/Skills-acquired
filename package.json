{
  "name": "sedona-19",
  "version": "17.0.0",
  "private": true,
  "description": "Личный проект «Седона» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "9.5.x",
    "browser-sync": "2.26.x",
    "del": "^5.1.0",
    "gh-pages": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^4.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-less": "4.0.x",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svgstore": "^7.0.1",
    "gulp-uglify": "^3.0.2",
    "gulp-webp": "^4.0.1",
    "picturefill": "^3.0.3",
    "stylelint": "10.1.x",
    "stylelint-config-htmlacademy": "0.1.x",
    "svg4everybody": "^2.1.9"
